"{_{ name }_}": {
    "parameters": {
        "autoFitMode": "fit",
        "showPoints": true,
        "legend": {
            "descOrder": false,
            "showHeader": true,
            "show": {_{ show_legend }_},
            "customSize": "auto",
            "position": "right-top",
            "inside": false
        },
        "axisMode": "{_{ axis_mode }_}",
        "tooltip": {
            "showBinLabel": true,
            "measures": "",
            "showNullValues": true,
            "showPercentage": true,
            "showDimensions": true,
            "showMeasures": true,
            "customizeTooltip": false,
            "dimensions": ""
        },
        "visualizationType": "{_{ vis_type }_}",
        "dashLine": {
            "measures": [
                "{_{ measure_name }_}"
            ],
            "showDashLine": {_{ show_dash }_}
        },
        "exploreLink": true,
        "title": {
            "fontSize": {_{ font_size }_},
            "subtitleFontSize": 11,
            "label": "{_{ chart_title }_}",
            "align": "left",
            "subtitleLabel": ""
        },
        "trellis": {
            "flipLabels": false,
            "showGridLines": true,
            "size": [
                100,
                100
            ],
            "enable": false,
            "type": "x",
            "chartsPerLine": 4
        },
        "showActionMenu": {_{ show_action }_},
        "fillArea": {_{ fill_area}_},
        "showZero": {_{ show_zero }_},
        "measureAxis2": {
            "sqrtScale": false,
            "showTitle": true,
            "showAxis": true,
            "title": " ",
            "customDomain": {
                "showDomain": false
            }
        },
        "measureAxis1": {
            "sqrtScale": false,
            "showTitle": true,
            "showAxis": true,
            "title": "{_{ title_1 }_}",
            "customDomain": {
                "showDomain": false
            }
        },
        "valueType": "none",
        "theme": "wave",
        "step": "{_{ step_name }_}",
        "dimensionAxis": {
            "showTitle": {_{ show_title }_},
            "customSize": "auto",
            "showAxis": {_{ show_axis }_},
            "title": "",
            "icons": {
                "useIcons": false,
                "iconProps": {
                    "fit": "cover",
                    "column": "",
                    "type": "round"
                }
            }
        },
        "drawArea": {
            "measure": "",
            "showDrawArea": false,
            "bounding1": "",
            "bounding2": ""
        }
    },
    "type": "chart"
}